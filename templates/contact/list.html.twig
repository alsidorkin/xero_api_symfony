<!DOCTYPE html>
<html>
<head>
    <title>Contacts List</title>
</head>
<body>
    <h1>{{ message }}</h1>
    <table border="1">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Contact_id</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            {% for contact in contacts %}
                <tr>
                    <td>{{ contact.firstName }}</td>
                    <td>{{ contact.lastName }}</td>
                    <td>{{ contact.emailAddress }}</td>
                     <td>{{ contact.contact_id }}</td>
                     <td>
                        
                        <a href="{{ path('form_list', { 'id': contact.contact_id }) }}">Edit</a>
    
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
    <div> <a href="{{ path('contacts_addform') }}">Add Contact</a></div>
    <div> <a href="{{ path('contacts_export_excel') }}">Export Exel</a></div>
</body>
</html>
